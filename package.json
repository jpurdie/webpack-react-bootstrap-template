{
  "name": "wbv_skeleton",
  "packageManager": "yarn@3.1.1",
  "version": "1.0.0",
  "description": "Webpack 5. Bootstrap 5. React. Skeleton/Template/Starter/Boilerplate",
  "main": "src/index.js",
  "scripts": {
    "caniuse": "npx browserslist@latest --update-db",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint src test --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npx prettier src test --write",
    "format": "yarn prettier:fix && yarn lint:fix",
    "test": "vitest --dir ./src --run --reporter verbose",
    "test:watch": "vitest",
    "watch": "cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js --watch",
    "dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js",
    "prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.2",
    "dayjs": "^1.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.17.8",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/bootstrap": "^5.2.5",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "latest",
    "mime-types": "^2.1.35",
    "mini-css-extract-plugin": "^2.6.0",
    "path": "^0.12.7",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "prettier": "2.7.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
